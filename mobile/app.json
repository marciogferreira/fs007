{
  "expo": {
    "name": "Sales Track",
    "slug": "sales_track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "br.com.salestrack",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#034166"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.haelio.salestracke"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#034166"
      },
      "package": "com.haelio.sales_track"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir o $(PRODUCT_NAME) usar sua localização?"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fda5baa-5bb8-4e97-94e8-cada9ef663d4"
      }
    },
    "owner": "haelio",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1fda5baa-5bb8-4e97-94e8-cada9ef663d4"
    }
  }
}
